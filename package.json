{
  "name": "@aaronchenwei/tslint-config",
  "description": "A sharable TSLint configuration",
  "version": "0.3.0",
  "license": "MIT",
  "main": "tslint.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.js"
  },
  "dependencies": {
    "tslint-consistent-codestyle": "^1.14.1",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "~5.2.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0"
  },
  "devDependencies": {
    "dictionary-en-us": "^2.0.0",
    "husky": "^1.1.3",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.6.0",
    "remark-frontmatter": "^1.3.0",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "remark-retext": "^3.1.1",
    "retext-english": "^3.0.1",
    "retext-repeated-words": "^1.2.2",
    "retext-sentence-spacing": "^2.0.3",
    "retext-spell": "^2.4.0",
    "retext-syntax-urls": "^1.0.1",
    "retext-usage": "^0.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6",
    "unified": "^7.0.1"
  },
  "peerDependencies": {
    "husky": "^1.1.0",
    "prettier": "^1.13.0",
    "pretty-quick": "^1.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.0"
  },
  "keywords": [
    "tslint",
    "typescript"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
